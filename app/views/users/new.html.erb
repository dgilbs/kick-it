<head>
</head>

<body>
  <%= form_for @user do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name%>
    <br><br>
    <%= f.label :email %>
    <%= f.text_field :email%>
    <br><br>
    <%= f.label :gender%>
    <%= f.select :gender, ["Male", "Female"]%>
    <br><br>
    <%= f.label :position%>
    <%= f.select :position, ["Forward", "Midfielder", "Defender", "Goalkeeper"]%>
    <br><br>
    <%= f.submit %>

  <%end%>
</body>